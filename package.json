{
  "name": "beauti-frontend",
  "version": "0.0.0",
  "scripts": {
    "prerender": "ng run beauti-frontend:prerender",
    "ng": "ng",
    "start": "ng serve -o --host 0.0.0.0 --port 4300",
    "build": "ng build",
    "build:prod": "yarn build:ssr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --configuration production --aot && ng run beauti-frontend:server:production --bundleDependencies all"
  },
  "private": false,
  "dependencies": {
    "@angular/cdk": "^10.2.7",
